{
    "$schema": "https://flagd.dev/schema/v0/flags.json",
    "flags": {
      "bg-color": {
        "state": "ENABLED",
        "variants": {
          "#D6D4D2": "#D6D4D2",
          "#73A53E": "#73A53E",
          "#FF7C00": "#FF7C00",
          "#D3D309": "#D3D309",
          "#4AB9D9": "#4AB9D9"
        },
        "defaultVariant": "#D6D4D2",
        "targeting": {
          "if": [
            {
              "==": [
                {
                  "var": "buildNumber"
                },
                0
              ]
            },
            "#73A53E",
            {
              "if": [
                {
                  "==": [
                    {
                      "var": "buildNumber"
                    },
                    1
                  ]
                },
                "#73A53E",
                {
                  "if": [
                    {
                      "==": [
                        {
                          "var": "buildNumber"
                        },
                        2
                      ]
                    },
                    "#FF7C00",
                    {
                      "if": [
                        {
                          "==": [
                            {
                              "var": "buildNumber"
                            },
                            3
                          ]
                        },
                        "#D3D309",
                        {
                          "if": [
                            {
                              ">=": [
                                {
                                  "var": "buildNumber"
                                },
                                4
                              ]
                            },
                            "#4AB9D9"
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
}
